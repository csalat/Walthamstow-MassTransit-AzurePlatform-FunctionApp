<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp3.1</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Walthamstow.MassTransit.AzurePlatform.FunctionApp</Title>
        <Description>Fork of MassTransit-Platform, only supports azure bus, all credits go for MassTransit project</Description>
        <PackageProjectUrl>https://github.com/csalat/Walthamstow-MassTransit-AzurePlatform/</PackageProjectUrl>
        <PackageVersion>7.1.7.1</PackageVersion>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
        <RepositoryUrl>https://github.com/csalat/Walthamstow-MassTransit-AzurePlatform/</RepositoryUrl>
        <IsPackable>true</IsPackable>
    </PropertyGroup>
	<ItemGroup>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.ApplicationInsights.AspNetCore" Version="2.17.0" />
        <PackageReference Include="Microsoft.AspNetCore.Hosting" Version="2.2.7" />
        <PackageReference Include="Microsoft.AspNetCore.Diagnostics.HealthChecks" Version="2.2.0" />
        <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="3.1.11" />
        <PackageReference Include="Microsoft.Extensions.Hosting" Version="5.0.0" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="5.0.1" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="MassTransit.AspNetCore" Version="7.1.7" />
        <PackageReference Include="MassTransit.Analyzers" Version="7.1.7" />
        <PackageReference Include="MassTransit.Azure.ServiceBus.Core" Version="7.1.7" />
        <PackageReference Include="MassTransit.WebJobs.ServiceBus" Version="7.1.7" />
        <PackageReference Include="Serilog.AspNetCore" Version="4.0.0" />
        <PackageReference Include="Serilog.Extensions.Hosting" Version="4.1.2" />
        <PackageReference Include="MassTransit.EntityFrameworkCore" Version="7.1.7" />
        <PackageReference Include="Walthamstow.MassTransit.AzurePlatform.Configs" Version="7.1.7" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.Azure.Functions.Extensions" Version="1.1.0" />
        <PackageReference Include="Microsoft.NET.Sdk.Functions" Version="3.0.11" />
    </ItemGroup>
</Project>
